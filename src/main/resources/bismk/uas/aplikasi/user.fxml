<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox alignment="CENTER" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bismk.uas.aplikasi.User">
   <children>
      <Pane prefHeight="364.0" prefWidth="752.0">
         <children>
   
             <TableView fx:id="userTable" layoutX="1.0" layoutY="68.0" prefHeight="242.0" prefWidth="751.0">
                 <columns>
                     <TableColumn prefWidth="50" text="ID">
                         <cellValueFactory>
                        <PropertyValueFactory property="id" />
                     </cellValueFactory>
                     </TableColumn>
                     <TableColumn prefWidth="100" text="Username">
                         <cellValueFactory>
                        <PropertyValueFactory property="username" />
                     </cellValueFactory>
                     </TableColumn>
                     <TableColumn prefWidth="100" text="Password">
                         <cellValueFactory>
                        <PropertyValueFactory property="password" />
                     </cellValueFactory>
                     </TableColumn>
                     <TableColumn prefWidth="100" text="Phone">
                         <cellValueFactory>
                        <PropertyValueFactory property="phone" />
                     </cellValueFactory>
                     </TableColumn>
                     <TableColumn prefWidth="150" text="Email">
                         <cellValueFactory>
                        <PropertyValueFactory property="email" />
                     </cellValueFactory>
                     </TableColumn>
                     <TableColumn prefWidth="150" text="Address">
                         <cellValueFactory>
                        <PropertyValueFactory property="address" />
                     </cellValueFactory>
                     </TableColumn>
                     <TableColumn prefWidth="100" text="Role">
                         <cellValueFactory>
                        <PropertyValueFactory property="role" />
                     </cellValueFactory>
                     </TableColumn>
                 </columns>
             </TableView>
         
             <Button layoutX="657.0" layoutY="322.0" onAction="#handleDelete" prefHeight="26.0" prefWidth="94.0" style="-fx-background-color: red;" text="Delete" textFill="WHITE">
               <font>
                  <Font name="Poppins Regular" size="12.0" />
               </font>
            </Button>
            <Label layoutX="1.0" layoutY="7.0" text="Data Users">
               <font>
                  <Font name="Poppins SemiBold" size="24.0" />
               </font>
            </Label>
            <Button layoutX="516.0" layoutY="322.0" onAction="#handleAddNewUser" prefHeight="26.0" prefWidth="133.0" style="-fx-background-color: green;" text="Add New Users" textFill="WHITE">
               <font>
                  <Font name="Poppins Regular" size="12.0" />
               </font>
            </Button>
             <Button layoutX="441.0" layoutY="322.0" onAction="#handleEdit" prefHeight="26.0" prefWidth="66.0" style="-fx-background-color: orange;" text="Edit" textFill="WHITE">
                 <font>
                     <Font name="Poppins Regular" size="12.0" />
                 </font>
             </Button>
         </children>
      </Pane>
   </children>
</HBox>
